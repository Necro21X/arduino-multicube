#include <Wire.h>
#include <Adafruit_RGBLCDShield.h>

Adafruit_RGBLCDShield lcd = Adafruit_RGBLCDShield();

int choice = 0;
uint8_t buttons;

String poper[] = {"Wave", "Cube"};

#define DATA_PIN 2
#define LATCH_PIN 4
#define CLOCK_PIN 3

#define NUM_SHIFT_PIN 70

int c = 0;
int p = 64;

const uint8_t numOfRegPins = NUM_SHIFT_PIN;

bool registers[numOfRegPins];
bool power = true;

void setup() {
  lcd.begin(16, 2);
  lcd.setBacklight(0x7);
  lcd.print(poper[choice]);
  lcd.setCursor(0, 0);
  pinMode(DATA_PIN, OUTPUT);
  pinMode(CLOCK_PIN, OUTPUT);
  pinMode(LATCH_PIN, OUTPUT);
  stopRegisters();
  shiftRegisters();
}

void loop() {
  buttons = lcd.readButtons();
  if (buttons & BUTTON_LEFT) {
    lcd.clear();
    choice = 0;
    lcd.print(poper[choice]);
    delay(500);
  }
  if (buttons & BUTTON_RIGHT) {
    lcd.clear();
    choice = 1;
    lcd.print(poper[choice]);
    delay(500);
  }
  if (choice == 0) {
    wave();
  }
  if (choice == 1) {
    cube();
  }
}

void stopRegisters() {
  for (int i = numOfRegPins - 1; i >= 0; i--) {
    if (i < 64) {
      registers[i] = LOW;
    }
    else {
      registers[i] = HIGH;
    }
  }
}

void shiftRegisters() {
  digitalWrite(LATCH_PIN, LOW);

  for (int i = numOfRegPins -1; i >= 0; i--) {
    digitalWrite(CLOCK_PIN, LOW);
    digitalWrite(DATA_PIN, registers[i]);
    digitalWrite(CLOCK_PIN, HIGH);
  }

  digitalWrite(LATCH_PIN, HIGH);
}

void setReg(int index, int value) {
  registers[index] = value;
}

void setCoord(int x, int y) {
  c = 1 * x + 8 * y;
  setReg(c, power);
}

void setPlane(int z) {
  p = z + 64;
  setReg(p, !power);
}

